<h1>New User</h1>

<%= form_for(@user) do |f|%>
<%= f.label "First Name"%>
<%= f.text_field :first_name %>
<%= f.label "Last Name" %>
<%= f.text_field :last_name %>
<%= f.label "Email" %>
<%= f.email_field :email %>
<%= f.label "Password"%>
<%= f.password_field :password %>

<%= f.submit "Create Account" %>
<% end %>